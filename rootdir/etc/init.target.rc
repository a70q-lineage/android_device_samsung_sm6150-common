on fs
    mount_all /vendor/etc/fstab.qcom --early
    #setprop ro.crypto.fde_algorithm AES-256-XTS
    chown root system /mnt/vendor/persist
    chmod 0771 /mnt/vendor/persist
    restorecon_recursive /mnt/vendor/persist
    mkdir /mnt/vendor/persist/data 0700 system system

    restorecon -R /mnt/vendor/efs

    restorecon -R /efs
    chown radio system /mnt/vendor/efs
    chmod 0771 /mnt/vendor/efs

    mkdir /mnt/vendor/persist/rfs 0700 system system

	setprop ro.product.model ${ro.boot.product.model}

on post-fs
    # super slow motion - FRC
	mkdir /data/vendor/mediacodec 0700 mediacodec mediacodec
    mkdir /data/vendor/mediacodec/fruc_files 0700 mediacodec mediacodec
    mkdir /data/vendor/mediacodec/fruc_assets 0700 mediacodec mediacodec
    copy /system/media/fruc_kernels.sm6150.bin /data/vendor/mediacodec/fruc_assets/fruc_kernels.sm6150.bin
    chown mediacodec mediacodec /data/vendor/mediacodec/fruc_assets/fruc_kernels.sm6150.bin

    # snap gpu kernel
    mkdir /data/vendor/snap 0755 cameraserver cameraserver
    copy /vendor/etc/snap_gpu_kernel_32.bin /data/vendor/snap/snap_gpu_kernel_32.bin
    copy /vendor/etc/snap_gpu_kernel_64.bin /data/vendor/snap/snap_gpu_kernel_64.bin
    chown cameraserver cameraserver /data/vendor/snap/snap_gpu_kernel_32.bin
    chown cameraserver cameraserver /data/vendor/snap/snap_gpu_kernel_64.bin
    chmod 0755 /data/vendor/snap/snap_gpu_kernel_32.bin
    chmod 0755 /data/vendor/snap/snap_gpu_kernel_64.bin

    # set RLIMIT_MEMLOCK to 64MB
    setrlimit 8 67108864 67108864
    write /dev/ipa 1

on late-fs
    wait_for_prop hwservicemanager.ready true
    mount_all /vendor/etc/fstab.qcom --late

service sec_nvm /vendor/bin/sec_nvm
    class core
    user system
    group system

